{
  "name": "gradual-frontend-assignment",
  "version": "1.0.0",
  "description": "Frontend Developer Assignment",
  "scripts": {
    "prepare": "is-ci || husky install",
    "preinstall": "npx only-allow pnpm",
    "start": "vite",
    "build": "tsc --noEmit && node --max_old_space_size=8196 ./node_modules/vite/bin/vite.js build",
    "preview": "vite preview --port 8888",
    "build:dev": "vite build --mode development",
    "test": "jest",
    "lint": "eslint --cache --ext .tsx,.ts,.js,.jsx .",
    "lint:fix": "pnpm prettier:fix && pnpm lint --fix",
    "prettier:check": "prettier --check ./",
    "prettier:fix": "prettier --write ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shijinyu/gradual-frontend-assignment.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shijinyu/gradual-frontend-assignment/issues"
  },
  "homepage": "https://github.com/shijinyu/gradual-frontend-assignment#readme",
  "browserslist": {
    "production": [
      "last 10 Chrome versions",
      "last 10 Edge versions",
      "last 10 Firefox versions",
      "last 3 Android versions",
      "last 3 ChromeAndroid versions",
      "last 2 Safari major versions",
      "last 2 iOS major versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Edge versions",
      "last 1 Firefox versions",
      "last 1 Safari major versions"
    ]
  },
